      "scope": "meta.definition.variable.name",
      "settings": { "foreground": "#FF9FF2" }
    },
    {
      "name": "Quoted/Template String",
      "scope": ["string.quoted", "string.template"],
      "settings": { "foreground": "#A6C9FF" }
    },
    {
      "name": "Regexp String",
      "scope": "string.regexp",
      "settings": { "foreground": "#A6C9FFF4" }
    },
    {
      "name": "Link Other String",
      "scope": "string.other.link",
      "settings": { "foreground": "#7FD0FF" }
    },
    {
      "name": "Docstring Quoted String",
      "scope": "string.quoted.docstring",
      "settings": { "foreground": "#76D8BB" }
    },
    {
