      "scope": "string.template meta.embedded",
      "settings": { "foreground": "#C5C8CA" }
    },
    {
      "name": "Entity Name",
      "scope": "entity.name",
      "settings": { "foreground": "#EB56DEEA" }
    },
    {
      "name": "Function Name Entity",
      "scope": "entity.name.function",
      "settings": { "foreground": "#7FD0FF" }
