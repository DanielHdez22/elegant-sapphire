      "settings": { "foreground": "#FF419A" }
    },
    {
      "name": "Class Support",
      "scope": "support.class",
      "settings": { "foreground": "#FFB81FEA" }
    },
    {
      "name": "Type Support",
      "scope": "support.type",
      "settings": { "foreground": "#7FD0FF" }
    },
    {
      "name": "Function Support",
      "scope": "support.function",
      "settings": { "foreground": "#7FD0FF" }
    },
    {
      "name": "Variable Support",
      "scope": "support.variable",
      "settings": { "foreground": "#9A98FF" }
    },
    {
      "name": "Property Name Type Support & Object Literal Key Meta",
      "scope": ["support.type.property-name", "meta.object-literal.key"],
      "settings": { "foreground": "#FF9FF2" }
    },
    {
      "name": "CSS Property Name Type Support",
      "scope": "support.type.property-name.css",
      "settings": { "foreground": "#C5C8CA" }
    },
    {
      "name": "Punctuation Definition",
      "scope": "punctuation.definition",
      "settings": { "foreground": "#0077FF" }
    },
    {
      "name": "Punctuation/Brace Meta",
      "scope": [
        "punctuation",
        "punctuation.definition.string",
        "punctuation.definition.parameters",
        "punctuation.terminator.statement",
        "punctuation.separator",
        "punctuation.section",
        "punctuation.accessor",
