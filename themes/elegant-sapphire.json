    {
      "name": "Tag Name Entity",
      "scope": "entity.name.tag",
      "settings": { "foreground": "#9A98FF", "fontStyle": "bold" }
    },
    {
      "name": "Type Name Entity & CSharp Storage Type",
      "scope": ["entity.name.type","storage.type.cs"],
      "settings": { "foreground": "#EB56DEEA" }
    },
    {
      "name": "Inherited Class Entity",
      "scope": "entity.other.inherited-class",
      "settings": { "foreground": "#EB56DEEA" }
    },
    {
      "name": "Other Attribute Name Entity",
      "scope": "entity.other.attribute-name",
      "settings": { "foreground": "#0077FF", "fontStyle": "bold" }
    },
    {
      "name": "ID Attribute Name Entity",
