      "scope": "punctuation.definition.list",
      "settings": { "foreground": "#2D99FFF6" }
    },
    {
      "name": "Heading Markup & Section Name Entity",
      "scope": ["markup.heading", "entity.name.section"],
      "settings": { "foreground": "#9A98FF" }
    },
    {
      "name": "Quote Markup",
      "scope": "markup.quote",
      "settings": { "foreground": "#BD52A2F6", "fontStyle": "italic" }
    },
    {
      "name": "Bold Markup",
      "scope": "markup.bold",
      "settings": { "foreground": "#9A98FF", "fontStyle": "bold" }
    },
    {
      "name": "Italic Markup",
      "scope": "markup.italic",
      "settings": { "foreground": "#7FD0FF", "fontStyle": "italic" }
    },
    {
      "name": "Inline Raw & Fenced Code Block Markup",
      "scope": ["markup.inline.raw", "markup.fenced_code.block"],
      "settings": { "foreground": "#2D99FFF6" }
