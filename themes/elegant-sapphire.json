  "semanticHighlighting": true,
  "tokenColors": [
    {
      "name": "Comment",
      "scope": "comment",
      "settings": { "foreground": "#7A8A9C", "fontStyle": "italic" }
    },
    {
      "name": "Keyword",
      "scope": "keyword",
      "settings": { "foreground": "#9A98FF", "fontStyle": "bold" }
    },
    {
      "name": "Operator Keyword",
      "scope": "keyword.operator",
      "settings": { "foreground": "#5590FF" }
    },
    {
      "name": "New/Expression/Logical/Delete Operator Keywords",
      "scope": [
        "keyword.operator.new",
        "keyword.operator.expression",
        "keyword.operator.logical",
        "keyword.operator.delete"
      ],
      "settings": { "foreground": "#9A98FF" }
    },
    {
      "name": "Unit Other Keyword",
      "scope": "keyword.other.unit",
      "settings": { "foreground": "#B18FFFF6" }
    },
    {
      "name": "Storage",
      "scope": "storage",
      "settings": { "foreground": "#9A98FF", "fontStyle": "bold" }
    },
    {
      "name": "Constant",
      "scope": "constant",
      "settings": { "foreground": "#69FFF3F6" }
    },
    {
      "name": "Escape Character Constant",
      "scope": "constant.character.escape",
      "settings": { "foreground": "#5691DD" }
    },
    {
      "name": "Variable",
      "scope": "variable",
      "settings": { "foreground": "#0077FF" }
    },
    {
      "name": "Language Variable",
      "scope": "variable.language",
      "settings": { "foreground": "#0077FF", "fontStyle": "italic" }
    },
    {
      "name": "Variable Name Entity",
      "scope": "entity.name.variable",
      "settings": { "foreground": "#64B2E0" }
    },
    {
      "name": "Readwrite Other Variable",
      "scope": "variable.other.readwrite",
      "settings": { "foreground": "#EB56DEEA" }
    },
    {
      "name": "Constant Other Variable",
      "scope": "variable.other.constant",
      "settings": { "foreground": "#299FFFF6" }
    },
    {
      "name": "Property Other Variable",
      "scope": "variable.other.property",
      "settings": { "foreground": "#FF549B" }
    },
    {
      "name": "Parameter Variable",
      "scope": "variable.parameter",
      "settings": { "foreground": "#FF9FF2", "fontStyle": "italic" }
    },
    {
      "name": "Struct Variable",
      "scope": "variable.struct",
      "settings": { "foreground": "#EB56DEEA" }
    },
    {
      "name": "Interface Variable",
      "scope": "variable.interface",
      "settings": { "foreground": "#EB56DEEA" }
    },
    {
